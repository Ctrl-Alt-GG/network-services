# {{ansible_managed}}
{
  "Control-agent": {
    "http-host": "127.0.0.1",
    "http-port": 8000,
    "control-sockets": {
      "dhcp4": {
        "socket-type": "unix",
        "socket-name": "/run/kea/kea4-ctrl-socket"
      }
    },
    "loggers": [
      {
        "name": "kea-ctrl-agent",
        "output-options": [ { "output": "/var/log/kea/kea-ctrl-agent.log" } ],
        "severity": "INFO",
        "debuglevel": 0
      }
    ]
  }
}
